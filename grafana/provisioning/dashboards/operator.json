        {
          "datasource": {"type": "postgres", "uid": "TimescaleDB"},
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  m.name as machine,\n  max(case when mt.metric_name = 'temperature' then mt.value end) as temp,\n  max(case when mt.metric_name = 'pressure' then mt.value end) as pressure,\n  max(case when mt.metric_name = 'vibration' then mt.value end) as vibration,\n  max(case when mt.metric_name = 'rpm' then mt.value end) as rpm\nFROM machines m\nLEFT JOIN metrics mt ON mt.machine_id = m.id AND mt.time > now() - interval '1 minute'\nWHERE m.status = 'active'\nGROUP BY m.name\nORDER BY m.name",
          "refId": "A"
        }
      ],
      "title": "ðŸ“‹ Machine Status Overview",
